@charset "utf-8";

// sass-lint:disable-all

{{#each sprites}}
$_sprites: map-merge($_sprites, (
	"{{../spritesheet_name}}-{{name}}": (
		"sheet": "{{../spritesheet_name}}",
		"properties": {{px.x}} {{px.y}} {{px.offset_x}} {{px.offset_y}} {{px.width}} {{px.height}} {{px.total_width}} {{px.total_height}} "{{{escaped_image}}}" "{{name}}",
		"is-retina": {{../options.is_retina}}
	)
));
{{/each}}
