{{#each sprites}}
$_sprite-{{../spritesheet_name}}-{{name}}: {{px.x}} {{px.y}} {{px.offset_x}} {{px.offset_y}} {{px.width}} {{px.height}} {{px.total_width}} {{px.total_height}} "{{{escaped_image}}}" {{name}};
{{/each}}

$_sprites: map-merge($_sprites, ({{spritesheet_name}}: (
	{{#each sprites}}
	$_sprite-{{../spritesheet_name}}-{{name}},
	{{/each}}
)));
$_sprite-scales: map-merge($_sprite-scales, ({{spritesheet_name}}: {{options.scale}}));
